
import random

secret = random.randint(1,20)

retries=0
while retries < 5:
    guess = int(input('Guess the number (1â€“20): '))
    if guess == secret:
        print('ðŸŽ‰ Correct! You guessed it.')
        break
    elif guess > secret:
        print('Too High')
    elif guess < secret:
        print('Too Low')

    retries+=1

if guess!=secret:
    print(f"ðŸ˜¢ Out of tries. The number was {secret}")